@model BIT285_SpicyShrimps.Models.Student

@{
    ViewData["Title"] = "StudentLogin";
}

<h2>secret Word</h2>

<h2>Type your secret word</h2>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="StudentLogin">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>

            <input type="button" value="OK" onclick="location.href='@Url.Action("StudentDashboard", "Login" )'" />

