@model BIT285_SpicyShrimps.Models.Student

@{
    ViewData["Title"] = "StudentLogin";
}

<h2>Student Login</h2>

<h2>What is your Section Code?</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="StudentLogin">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SectionCode" class="control-label"></label>
                <input asp-for="SectionCode" class="form-control" />
                <span asp-validation-for="SectionCode" class="text-danger"></span>
            </div>

            <input type="button" value="Submit" onclick="location.href='@Url.Action("VerifyTeacher", "Login" )'" />


